.navbar.navbar-inverse.navbar-fixed-top{role: 'navigation'}
  .container
    .navbar-header
      %button.navbar-toggle.collapsed{'data-target' => '.navbar-collapse', 'data-toggle' => 'collapse', type: 'button'}
        %span.sr-only Toggle navigation
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      = link_to 'Project name', root_path , class: 'navbar-brand'
    -if user_signed_in?
      .navbar-collapse.collapse
        %ul.nav.navbar-nav
          = render 'layouts/navigation_left'
        %ul.navbar-form.navbar-right
          = render 'layouts/navigation_right'
